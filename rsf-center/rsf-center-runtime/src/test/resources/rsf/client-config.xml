<?xml version="1.0" encoding="UTF-8"?>
<config xmlns="http://project.hasor.net/hasor/schema/main">
    <!-- 框架配置，包扫描范围 -->
    <hasor.environmentVar>
        <!-- 注册中心 -->
        <RSF_CENTER_URL>rsf://127.0.0.1:7001</RSF_CENTER_URL>
        <!-- RSF对外提供服务的IP -->
        <RSF_SERVICE_IP>local</RSF_SERVICE_IP>
        <!-- RSF对外提供服务的服务端口 -->
        <RSF_SERVICE_PORT>8001</RSF_SERVICE_PORT>
        <!-- Telnet 控制台端口 -->
        <RSF_CONSOLE_PORT>8002</RSF_CONSOLE_PORT>
    </hasor.environmentVar>
</config>