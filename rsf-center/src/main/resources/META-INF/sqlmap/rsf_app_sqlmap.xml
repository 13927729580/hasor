<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sp_rsf_class">

	<resultMap id="appDOMap" type="net.hasor.rsf.center.domain.entity.AppDO">
		<result property="appID"        column="app_id" />
		<result property="appCode"      column="app_code" />
		<result property="appName"      column="app_name" />
		<result property="accessKey"    column="app_accessKey" />
		<result property="accessSecret" column="app_accessSecret" />
		<result property="onwer"        column="app_onwer" />
		<result property="contactUsers" column="app_contactUsers" />
		<result property="description"  column="app_desc" />
		<result property="createTime"   column="app_create_time" />
		<result property="modifyTime"   column="app_modify_time" />
	</resultMap>
	<sql id="appDO_allColumns">
		app_id,
		app_code,
		app_name,
		app_accessKey,
		app_accessSecret,
		app_onwer,
		app_contactUsers,
		app_desc,
		app_create_time,
		app_modify_time
	</sql>

	<select id="appDO_getALL" resultMap="appDOMap">
		select
			<include refid="appDO_allColumns" />
		from
			RSF_APP
	</select>
	
	<insert id="createByAppDO" useGeneratedKeys="true" keyProperty="appID" parameterType="net.hasor.rsf.center.domain.entity.AppDO">
		insert into RSF_APP(
			app_id,
			app_code,
			app_name,
			app_accessKey,
			app_accessSecret,
			app_onwer,
			app_contactUsers,
			app_desc,
			app_create_time,
			app_modify_time
		) values(
			#{appID},
			#{appCode},
			#{appName},
			#{accessKey},
			#{accessSecret},
			#{onwer},
			#{contactUsers},
			#{description},
			now(),
			now()
		);
		<selectKey keyProperty="appID" resultType="long">
			CALL IDENTITY()
		</selectKey> 
	</insert>
</mapper>